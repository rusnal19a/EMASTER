var urlku="https://master.bkd.jatimprov.go.id/media.php?module=jab_ptk&id_skpd=bb7d715110565782e212a11d2fcc5b92&id=b5e08fbbea80d0296a47b8a8f30e04eb";

// Ambil elemen input berdasarkan ID
const inputField = document.querySelector('.inpku');

// Tambahkan event listener untuk menangkap input
inputField.addEventListener('input', function() {
// Tampilkan hasil ketikan di console
hasil=dataku.filter(user => user.nama.toLowerCase().includes(inputField.value.toLowerCase()));
if(hasil.length==1){
var urlawal=window.location.href;
  console.log(hasil[0].nama);
// Mendapatkan elemen div dengan class "urlku"
var divUrlku = document.querySelector('.urlku');
divUrlku.textContent="";
// Membuat elemen <a>
	var link = document.createElement('a');

	// Menetapkan atribut href dan teks untuk elemen <a>
		//link.href = window.location.href;
	const urlObj = new URL(urlawal);
	urlObj.searchParams.set('id', hasil[0].id);
	link.href = urlObj;


		link.textContent = hasil[0].nama;

		// Menambahkan elemen <a> ke dalam div
			divUrlku.appendChild(link);
  }
  
});

var inputdlm = document.querySelector('.inpku');
    
 inputdlm.addEventListener('keydown', function(event) {
        // Mengecek apakah tombol yang ditekan adalah Enter
        if (event.key === 'Enter') {
            // Mencegah perilaku default (jika diperlukan)
            event.preventDefault();
            // Mengklik link
            var linkdlm = document.querySelector('.urlku a');
          linkdlm.click();
            
        }
 })
