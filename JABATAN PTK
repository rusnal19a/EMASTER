// Create the main div element
const fixedDiv = document.createElement('div');
fixedDiv.id = 'fixedku';
fixedDiv.style.height = '100px';
fixedDiv.style.width = '100px';
fixedDiv.style.background = 'red';
fixedDiv.style.position = 'fixed';
fixedDiv.style.bottom = '10px';
fixedDiv.style.right = '0px';
fixedDiv.style.zIndex = '999';

// Create the input element
const inputElement = document.createElement('input');
inputElement.type = 'text';
inputElement.value = 'teks';
inputElement.className = 'inpku';

// Create the inner div for the link
const urlDiv = document.createElement('div');
urlDiv.className = 'urlku';

// Create the anchor element
const anchorElement = document.createElement('a');
anchorElement.href = '';
anchorElement.textContent = 'dsfd';

// Append the anchor to the urlDiv
urlDiv.appendChild(anchorElement);

// Append the input and urlDiv to the fixedDiv
fixedDiv.appendChild(inputElement);
fixedDiv.appendChild(urlDiv);

// Append the fixedDiv to the body
document.body.appendChild(fixedDiv);



